{
  "name": "puraisin",
  "version": "0.0.1",
  "description": "Uusi hieno slack-app Kosmos ry:lle",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.1.0",
  "scripts": {
    "build:server": "BABEL_ENV=server babel src --delete-dir-on-start --extensions \".ts,.tsx\" --out-dir ./dist-server",
    "build:client": "rm -rf dist/bundle && rollup -c",
    "build": "yarn build:server && yarn build:client",
    "deploy": "docker build -t kosmosry/puraisin . && docker push kosmosry/puraisin",
    "db:start": "docker-compose -f puraisin-db.yml up -d",
    "db:stop": "docker-compose -f puraisin-db.yml down",
    "watch:client": "NODE_ENV=dev rollup -c -w",
    "watch:server": "BABEL_ENV=server NODE_ENV=dev babel-watch src/server/app.ts --extensions \".ts,.tsx\"",
    "watch": "run-p -l \"watch:*\"",
    "lint": "yarn eslint 'src/**/*.{ts,tsx}' --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@slack/web-api": "^6.7.2",
    "connect-pg-simple": "^7.0.0",
    "dayjs": "^1.11.3",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "node-pg-migrate": "^6.2.1",
    "passport": "^0.5.3",
    "passport-oauth2": "^1.6.1",
    "pg": "^8.7.3",
    "pg-promise": "^10.11.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^3.1.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/config": "^0.0.41",
    "@types/connect-pg-simple": "^4.2.4",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/node": "^16.11.39",
    "@types/passport": "^1.0.9",
    "@types/passport-oauth2": "^1.4.11",
    "@types/pg": "^8.6.5",
    "@types/pg-promise": "^5.4.3",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/styled-components": "^5.1.25",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "babel-plugin-styled-components": "^1.13.3",
    "babel-watch": "^7.7.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.0",
    "rollup": "^2.75.6",
    "rollup-plugin-output-manifest": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  }
}
